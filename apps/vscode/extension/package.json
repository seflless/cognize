{
	"name": "cognize-vscode",
	"description": "The cognize extension for VS Code.",
	"version": "2.0.8",
	"private": true,
	"packageManager": "yarn@3.5.0",
	"author": {
		"name": "Francois Laberge",
		"email": "francoislaberge@gmail.com"
	},
	"homepage": "https://github.com/seflless/cognize",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/seflless/cognize"
	},
	"bugs": {
		"url": "https://github.com/seflless/cognize/issues"
	},
	"keywords": [
		"documentation",
		"code",
		"understanding",
		"diagrams",
		"drawing",
		"app",
		"development",
		"whiteboard",
		"canvas",
		"infinite"
	],
	"displayName": "Cognize",
	"publisher": "tldraw-org",
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#1d1d1d",
		"theme": "dark"
	},
	"categories": [
		"Visualization"
	],
	"engines": {
		"vscode": "^1.75.1"
	},
	"activationEvents": [],
	"browser": "./dist/web/extension.js",
	"main": "./dist/web/extension.js",
	"extensionKind": [
		"workspace"
	],
	"contributes": {
		"customEditors": [
			{
				"viewType": "cognize.cog",
				"displayName": "Cognize",
				"selector": [
					{
						"filenamePattern": "*.cog"
					},
					{
						"filenamePattern": "*.cog.json"
					}
				]
			}
		],
		"keybindings": [
			{
				"key": "ctrl+shift+d",
				"mac": "cmd+shift+d",
				"title": "Toggle Dark Mode",
				"command": "cognize.cog.toggleDarkMode",
				"category": "cognize",
				"when": "resourceExtname == .cog"
			},
			{
				"key": "ctrl+numpad_add",
				"mac": "cmd+numpad_add",
				"title": "Zoom In",
				"command": "cognize.cog.zoomIn",
				"category": "cognize",
				"when": "resourceExtname == .cog"
			},
			{
				"key": "ctrl+=",
				"mac": "cmd+=",
				"title": "Zoom In",
				"command": "cognize.cog.zoomIn",
				"category": "cognize",
				"when": "resourceExtname == .cog"
			},
			{
				"key": "ctrl+numpad_subtract",
				"mac": "cmd+numpad_subtract",
				"title": "Zoom Out",
				"command": "cognize.cog.zoomOut",
				"category": "cognize",
				"when": "resourceExtname == .cog"
			},
			{
				"key": "ctrl+-",
				"mac": "cmd+-",
				"title": "Zoom Out",
				"command": "cognize.cog.zoomOut",
				"category": "cognize",
				"when": "resourceExtname == .cog"
			},
			{
				"key": "ctrl+numpad0",
				"mac": "cmd+numpad0",
				"title": "Reset Zoom",
				"command": "cognize.cog.resetZoom",
				"category": "cognize",
				"when": "resourceExtname == .cog"
			}
		],
		"commands": [
			{
				"command": "cognize.cog.new",
				"title": "New Cognize file",
				"category": "cognize"
			}
		]
	},
	"vsce": {
		"dependencies": false
	},
	"scripts": {
		"dev": "tsx scripts/dev.ts",
		"build": "cd ../editor && yarn build && cd ../extension && tsx scripts/build.ts",
		"package": "yarn build && tsx scripts/package.ts",
		"publish": "vsce publish",
		"lint": "yarn run -T tsx ../../../scripts/lint.ts",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf editor && rm -rf temp & yarn"
	},
	"devDependencies": {
		"@tldraw/editor": "workspace:*",
		"@tldraw/file-format": "workspace:*",
		"@tldraw/store": "workspace:*",
		"@types/fs-extra": "^11.0.1",
		"@types/node-fetch": "^2.6.2",
		"@types/vscode": "^1.75.1",
		"@typescript-eslint/eslint-plugin": "^5.10.2",
		"@typescript-eslint/parser": "^5.10.2",
		"assert": "^2.0.0",
		"esbuild": "^0.16.7",
		"fs-extra": "^11.1.0",
		"lazyrepo": "0.0.0-alpha.27",
		"lodash": "^4.17.21",
		"mocha": "^9.1.1",
		"process": "^0.11.10",
		"ts-loader": "^9.2.5",
		"tslib": "^2.4.0",
		"tsx": "^3.12.6",
		"vsce": "^2.15.0"
	},
	"gitHead": "4b1137849ad07da36fc8f0f19cb64e7535a79296",
	"dependencies": {
		"nanoid": "4.0.2",
		"node-fetch": "^2.0.0"
	}
}
